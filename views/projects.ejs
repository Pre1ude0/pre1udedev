<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="UTF-8">
    <link rel="icon" href="https://larssj.dev/img/pfp-favico.png">
    <title>Larssj.dev/Projects</title>
    <meta property="og:title" content="LarssJ.dev/projects">
    <meta property="og:description" content="My projects">
    <meta property="og:image" content="https://larssj.dev/img/pfp-favico.png">
    <meta property="og:url" content="https://larssj.dev/projects">
    <meta property="og:type" content="website">
    <meta name="theme-color" content="#ff773d">
    <meta name="msapplication-TileColor" content="#ff773d">

    <link rel="stylesheet" href="../css/projects.css">
    <link rel="stylesheet" href="../css/top-bar.css">
</head>

<body>
    <script src="https://kit.fontawesome.com/bd75686a6c.js" crossorigin="anonymous"></script>

    <div class="top-bar" id="top-bar-div"></div>

    <div class="main">
        <div class="main-title">
            <h1>My projects</h1>
        </div>
        <div class="projects-container">
            <div class="mac-window">
                <div class="title-bar">
                    <div class="window-buttons">
                        <div class="window-button close"></div>
                        <div class="window-button minimize"></div>
                        <div class="window-button expand"></div>
                    </div>
                    <div class="window-title">LarssJ.dev</div>
                </div>
                <div class="mac-card">
                    <div class="project-card-title">LarssJ.dev</div>
                    <div class="project-card-description">My personal website</div>
                    <div class="project-card-buttons">
                        <div class="project-card-button" onclick="window.open('/', '_blank')">
                            <i class="fa-solid fa-globe"></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="mac-window">
                <div class="title-bar">
                    <div class="window-title">Larss_Bot</div>
                    <div class="window-buttons">
                        <div class="window-button close"></div>
                        <div class="window-button minimize"></div>
                        <div class="window-button expand"></div>
                    </div>
                </div>
                <div class="mac-card">
                    <div class="project-card-title">Larss_Bot</div>
                    <div class="project-card-description">My hobby discord bot project</div>
                    <div class="project-card-buttons">
                        <div class="project-card-button"
                            onclick="window.open('https://larssbot.onrender.com/', '_blank')"><i
                                class="fa-solid fa-globe"></i></div>
                        <div class="project-card-button"
                            onclick="window.open('https://github.com/Pre1ude0/Larss_Bot/', '_blank')"><i
                                class="fa-brands fa-github"></i></div>
                        <div class="project-card-button"
                            onclick='window.open("https://discord.gg/TReMEyBQsh", "_blank")'><i
                                class=" fa-brands fa-discord"></i></div>
                    </div>
                </div>
            </div>
            <div class="mac-window">
                <div class="title-bar">
                    <div class="window-title">Discord RPC template</div>
                    <div class="window-buttons">
                        <div class="window-button close"></div>
                        <div class="window-button minimize"></div>
                        <div class="window-button expand"></div>
                    </div>
                </div>
                <div class="mac-card">
                    <div class="project-card-title">Discord rich presence template</div>
                    <div class="project-card-description">A python discord rich presence template using pypresence</div>
                    <div class="project-card-buttons">
                        <div class="project-card-button"
                            onclick="window.open('https://github.com/Pre1ude0/Discord-rich-presence-python', '_blank')">
                            <i class="fa-brands fa-github"></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="mac-window">
                <div class="title-bar">
                    <div class="window-title">UpGranT</div>
                    <div class="window-buttons">
                        <div class="window-button close"></div>
                        <div class="window-button minimize"></div>
                        <div class="window-button expand"></div>
                    </div>
                </div>
                <div class="mac-card">
                    <div class="project-card-title">UpGrandTour</div>
                    <div class="project-card-description">In dev eu project archive site</div>
                    <div class="project-card-buttons">
                        <div class="project-card-button" onclick="window.open('https://upgrandtour.eu', '_blank')"><i
                                class="fa-solid fa-globe"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="../js/top-bar.js"></script>
</body>

</html>